<span class="label label-success" data-toggle="tooltip" data-placement="top" title="Number of public observations"><span class="glyphicon glyphicon-ok-circle" style="color:white;"></span> <span class="pubcount" data-itemid1=<%= itemid1 %> data-model1=<%= params[:controller] %>><span class="glyphicon glyphicon-refresh spinning"></span></span></span>

<% if logged_in? && current_user.editor? %>
	<span class="label label-danger" data-toggle="tooltip" data-placement="top" title="Private observations"><span class="glyphicon glyphicon-remove-circle" style="color:white;"></span> <span class="pricount" data-itemid1=<%= itemid1 %> data-model1=<%= params[:controller] %>><span class="glyphicon glyphicon-refresh spinning"></span></span></span>
<% end %>
